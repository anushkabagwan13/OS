//copying elements from sourcevector to anothervector

#include<iostream>
#include<vector>
using namespace std;

int main(){
    vector<int> anotherVector;
    vector<int>sourceVector={40,50,60};
    
    anotherVector.insert(anotherVector.end(),sourceVector.begin(),
    sourceVector.end());
    
    cout<<"Elements of another vector:";
    for(int i=0;i<anotherVector.size();++i){
        cout<<anotherVector[i]<<" ";
    }
    cout<<endl;
    return 0;
}
